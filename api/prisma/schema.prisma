generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model Item {
    skuId Int @id
    title String
    brand String
    model String
    specs Json

    prices ItemPrice[]
}

model ItemPrice {
    id Int @id @default(autoincrement())
    price Float
    asOf DateTime
    item Item @relation(fields: [itemId], references: [skuId])
    itemId Int
}


